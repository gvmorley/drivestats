        <search>
          <query>
            <![CDATA[index=$tok_step_1_index$ model=HGST*
            | stats distinct_count(model)]]>
          </query>

          <earliest>$tok_time_earliest$</earliest>
          <latest>$tok_time_latest$</latest>

          <progress>
            <eval token="tok_step_1_dc_hgst_duration"><![CDATA[round($job.runDuration$,6)]]></eval>
            <set token="tok_step_1_dc_hgst_colour">0</set>
            <eval token="tok_step_1_total_duration"><![CDATA[round($tok_step_1_dc_hgst_duration$+$tok_step_1_dc_hitachi_duration$+$tok_step_1_dc_wdc_duration$+$tok_step_1_dc_st_duration$+$tok_step_1_dc_samsung_duration$+$tok_step_1_dc_toshiba_duration$,6)]]></eval>
          </progress>

          <done>
            <eval token="tok_step_1_dc_hgst_duration"><![CDATA[round($job.runDuration$,6)]]></eval>
            <set token="tok_step_1_dc_hgst_colour">1</set>
            <eval token="tok_step_1_total_duration"><![CDATA[round($tok_step_1_dc_hgst_duration$+$tok_step_1_dc_hitachi_duration$+$tok_step_1_dc_wdc_duration$+$tok_step_1_dc_st_duration$+$tok_step_1_dc_samsung_duration$+$tok_step_1_dc_toshiba_duration$,6)]]></eval>
          </done>

        </search>

        <option name="colorBy">value</option>
        <option name="colorMode">block</option>
        <option name="drilldown">none</option>
        <option name="numberPrecision">0</option>
        <option name="rangeColors">["0x53a051","0x53a051"]</option>
        <option name="rangeValues">[0]</option>
        <option name="trendColorInterpretation">standard</option>
        <option name="trendDisplayMode">absolute</option>
        <option name="unitPosition">after</option>
        <option name="useColors">$tok_step_1_dc_hgst_colour$</option>
        <option name="useThousandSeparators">1</option>

